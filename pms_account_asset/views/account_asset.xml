<odoo>

    <record id="view_account_asset_form" model="ir.ui.view">
        <field name="name">account.asset.property.form</field>
        <field name="model">account.asset</field>
        <field
            name="inherit_id"
            ref="account_asset_management.account_asset_view_form"
        />
        <field name="groups_id" eval="[(4, ref('pms_base.group_pms_user'))]" />
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="property_id" />
            </field>
        </field>
    </record>

</odoo>
